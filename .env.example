# Multi-Tenant SDK Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/myapp
REDIS_URL=redis://localhost:6379
MONGODB_URL=mongodb://localhost:27017/myapp

# Supabase Configuration (Phase 1)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Work OS Integration
WORKOS_API_KEY=your-workos-api-key
WORKOS_WEBHOOK_SECRET=your-webhook-secret

# Analytics Configuration (Phase 3)
ANALYTICS_ENABLED=true
ANALYTICS_BUFFER_SIZE=1000

# Cache Configuration (Phase 3)
CACHE_ENABLED=true
CACHE_TTL=300

# Multi-Region Configuration (Phase 3)
MULTI_REGION_ENABLED=false
PRIMARY_REGION=us-east-1

# Security Configuration
RATE_LIMIT_ENABLED=true
MAX_REQUESTS_PER_MINUTE=1000

# Development Settings
NODE_ENV=development
LOG_LEVEL=info
