{
  "name": "@tin/tin-multi-tenant-sdk",
  "version": "1.0.0-alpha.1",
  "description": "TIN Multi-Tenant SDK - Enterprise-grade multi-tenant SaaS SDK with database abstraction, intelligent caching, and global deployment",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build:clean && npm run test:ci && npm run lint",
    "prepack": "npm run build",
    "release": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "keywords": [
    "multi-tenant",
    "saas",
    "supabase",
    "sdk",
    "work-os",
    "tenant-isolation"
  ],
  "author": "TIN Developer",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "pg": "^8.11.3",
    "mysql2": "^3.6.3",
    "mongodb": "^6.3.0",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4",
    "eventemitter3": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.5",
    "@types/pg": "^8.10.7",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "PHASE1_SUMMARY.md",
    "PHASE2_SUMMARY.md",
    "PHASE3_SUMMARY.md",
    "MIGRATION_GUIDE.md",
    ".env.example"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tin-network/tin-multi-tenant-sdk"
  },
  "bugs": {
    "url": "https://github.com/tin-network/tin-multi-tenant-sdk/issues"
  },
  "homepage": "https://github.com/tin-network/tin-multi-tenant-sdk#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
